LXI B,0000H ; FOR COUNTING QUOTIENT
LHLD 9000H ; LOAD DIVISOR IN HL FROM 9000H
XCHG ;EXCHANGE DIVISOR WITH DE REG. PAIR
LHLD 9002H ; HL=DIVIDEND
AGAIN:
MOV A,L ; MOV L TO A
SUB E ; SUB 
MOV L,A
MOV A,H
SBB D
MOV H,A
JC DOWN
INX B ; INCREASE QUOTIENT
JMP AGAIN
DOWN:
DAD D ; RESTORING DATA IF BORROW HAS BEEN TAKEN
SHLD 9003H
MOV H,B
MOV L,C
SHLD 9101H
HLT