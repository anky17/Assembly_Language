LXI B,0000H ; FOR CARRY BIT
LHLD 9000H ; LOAD MULTIPLICANT
SPHL ; TRANSFER MULTIPLICAND TO STACK
LHLD 9002H ; LOAD HL REG. WITH MULTIPLIER
XCHG ; EXCHANGE CONT. OF HL WITH DE FOR COUNTER
LXI H,0000H ; INITIALIZE HL=0000H TO INITIALIZE GV
UP:
DAD SP ; HL=HL+SP
JNC DOWN ; IF CARRY FLAG IS NOT SET GO DOWN
INX B ; INCREASE CARRY BIT
DOWN: 
DCX D ; DECREASE COUNTER
MOV A,E ; MOVE CONT. OF REG. E TO A
ORA D ; TO CHECK DE=0 OR NOT
JNZ UP ; IF ZERO FLAG IS NOT SET GO UP
SHLD 9100H ; STORE CONT. OF HL AT 9100H
MOV H,B ; MOVE CONT. OF REG. B TO H
MOV L,C ; MOVE CONT. OF REG. C TO L
SHLD 9102H ;STORE CONT. OF HL AT 9102H
HLT  ; END OF PROGRAM

